#include <stdio.h>
#include <string.h>
int main()
{
    char str[100];
    int length, start;
    printf("Enter a string : ");
    scanf("%s", str);
    length = strlen(str);
    start = length / 2 - (length + 1) % 2;
    printf("Middle characters: ");
    for (int i=start;i<=start+(length+1)%2;i++)
    {
        printf("%c",str[i]);
    }
    return 0;
}
