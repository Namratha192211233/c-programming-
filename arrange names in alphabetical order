#include <stdio.h>
#include <string.h>
main(void)
{
	int i,j,n;
	char a[100][100],s[100];
	printf("Enter the number of names : ");
	scanf("%d",&n);
	printf("Enter names : \n");
	for(i=0;i<n;i++)
	{
		scanf("%s",a[i]);
	}
	for(i=0;i<n;i++)
	{
		for(j=i+1;j<n;j++)
		{
			if(strcmp(a[i],a[j])>0)
			{
				strcpy(s,a[i]);
				strcpy(a[i],a[j]);
				strcpy(a[j],s);
			}
		}
	}
	printf("The alphabetical order of names are : \n");
	for(i=0;i<n;i++)
	{
		printf("%s\n",a[i]);
	}
}
